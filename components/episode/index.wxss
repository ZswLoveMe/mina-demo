/* components/epsoide/epsoide.wxss */
.container{
    display: inline-flex;
    flex-direction: row;
    height: 60rpx;
}
.left-wrapper{
    display: flex;
    flex: row;
    align-items: baseline;
}
.left-wrapper > text:nth-child(1){
    font-size: 32rpx;
}
.left-wrapper > text:nth-child(2){
    font-weight: 800;
    font-size: 60rpx;
    line-height: 60rpx;
    margin-right:14rpx;
}
.line{
    height: 44rpx;
    margin-right: 14rpx;
    border: 1rpx solid black;
}
.right-wrapper{
    height: 60rpx;
    display: flex;
    flex-direction: column;
}
.right-wrapper > text:nth-child(1){
    font-size: 24rpx;
    line-height: 24rpx;
}